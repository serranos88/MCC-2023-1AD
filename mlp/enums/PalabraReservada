/*Palabras reservadas para la sintaxis del MLP */
public enum PalabraReservada {
LEER("leer"),  IMPRIME("imprime"),SI("si"), SINO("sino"),MIENTRAS("mientras");
	
	private String comando;
	
	private PalabraReservada(String comando) {
		this.comando = comando;
	}
	public String toString() {
		return this.comando;
	}
	
	static public boolean isPalabraReservada(String linea) {
		for(int i=0; i<PalabraReservada.values().length;i++) {
			if(linea.contains(PalabraReservada.values()[i].toString())) {
				return true;
			}
		}
		return false;
			
	}
}